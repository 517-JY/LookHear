<html>
  <body>
    <form ref='uploadForm'
      id='uploadForm'
      action='/form/upload'
      method='post'
      encType="multipart/form-data">
        <input type="file" name="sampleFile" />
        <input type="text" name="partName" />
        <input type='submit' value='Upload!' />
    </form>
    <ul class="piece">
        <li class="piece">
          <span><%= pieceTitle %></span>
          <span><%= pieceOwner %></span>
          <span><%= pieceParts %></span>
        </li>
    </ul>
  </body>
</html>
